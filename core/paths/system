  "0 z.env    '${HOME}/.zsh.d/.zshenv'"\
  "0 z.als    '${HOME}/.zsh.d/.zaliases'"\
  "0 v        '${HOME}/.vimrc'"\
  "0 e        '${HOME}/.emacs.d/init.el'"\
  "0 t        '${HOME}/.tmux.conf'"\
  "1 s        '/etc/ssh/sshd_conf'"\
  "0 s.conf   '${HOME}/.ssh/config'"
