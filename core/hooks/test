[info]
AUTHOR "TADOKORO Saneyuki <saneyan@mail.gfunction.com>"
VERSION "0.1.0"

[enable]
ECHO -n "Hook test: "
ECHO "enable"
RUN uptime
LINK @dmgr.test
LINK @dmgr.test.option ~/dmgr.test

[disable]
ECHO -n "Hook test: "
ECHO "disable"
RUN uptime
UNLINK @dmgr.test
UNLINK @dmgr.test.option

[update]
REARCH disable ECHO UNLINK
REARCH enable ECHO LINK
