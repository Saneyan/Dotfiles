[enable]
# Link local xorg setting files.
LINK @xorg.d
LINK ~/.xorg.d/xsession ~/.xsession
LINK ~/.xorg.d/xsession ~/.xinitrc

# Link gvimrc
LINK @vim/gvimrc ~/.gvimrc

[disable]
UNLINK @xorg.d
UNLINK ~/.xorg.d/xsession
UNLINK @vim/gvimrc

[update]
REARCH disable ECHO UNLINK
REARCH enable ECHO LINK
